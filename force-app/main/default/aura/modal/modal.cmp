<aura:component
  implements="forceCommunity:availableForAllPageTypes, flexipage:availableForAllPageTypes, 
flexipage:availableForRecordHome, 
force:hasRecordId"
  access="global"
>
  <ui:button label="openmodal" press="{!c.openmodal}" />
  <div
    role="dialog"
    tabindex="-1"
    aria-labelledby="header43"
    aura:id="Modalbox"
    class="slds-modal"
  >
    <div class="slds-modal__container" style="width:90%; max-width: 90%;">
      <div class="slds-modal__header">
        <button
          class="slds-button slds-modal__close slds-button--icon-inverse"
          title="Close"
          onclick="{!c.closeModal}"
        >
        X
        </button>
        <h2 id="header43" class="slds-text-heading--medium">Test Modal</h2>
      </div>
      <div class="slds-modal__content slds-p-around--medium" style="height: 60%;">
        <div>
          <c:ExampleProcess />
          <c:customFlow />
        </div>
      </div>
      <div class="slds-modal__footer"></div>
    </div>
  </div>
  <div class="slds-backdrop" aura:id="Modalbackdrop"></div>
</aura:component>
