<aura:component>
  <!-- Attributes -->
  <aura:attribute name="firstCheckboxGroup" type="List" />
  <aura:attribute name="secondCheckboxGroup" type="List" />
  <aura:attribute name="thirdCheckboxGroup" type="List" />

  <!-- Events -->
  <aura:handler name="init" value="{!this}" action="{!c.loadCheckboxGroups}" />
  <table>
    <tr>
      <td style="vertical-align: baseline;">
        <c:checkboxGroup
          value="{!v.firstCheckboxGroup}"
          title="Customer Type"
        />
      </td>
      <td style="vertical-align: baseline;">
        <c:checkboxGroup value="{!v.secondCheckboxGroup}" title="Order Type" />
      </td>
      <td style="vertical-align: baseline;">
        <c:checkboxGroup value="{!v.thirdCheckboxGroup}" title="Payment Plan" />
      </td>
    </tr>
  </table>
</aura:component>
